{
    "name": "webfont",
    "version": "2.0.1",
    "description": "Generator of webfonts from svg icons",
    "license": "MIT",
    "author": "itgalaxy <development@itgalaxy.company>",
    "contributors": [
        {
            "name": "Alexander Krasnoyarov",
            "email": "alexander.krasnoyarov@itgalaxy.company",
            "url": "https://vk.com/sterling_archer"
        }
    ],
    "repository": {
        "type": "https",
        "url": "https://github.com/itgalaxy/webfont"
    },
    "keywords": [
        "cli",
        "standalone",
        "font",
        "fonts",
        "webfont",
        "webfonts",
        "svg",
        "ttf",
        "woff",
        "woff2"
    ],
    "bugs": {
        "url": "https://github.com/itgalaxy/webfont/issues"
    },
    "main": "dist/index.js",
    "bin": "dist/cli.js",
    "files": [
        "CHANGELOG.md",
        "LICENSE",
        "dist",
        "templates",
        "!**/__tests__"
    ],
    "dependencies": {
        "globby": "^6.0.0",
        "meow": "^3.3.0",
        "mkdirp": "^0.5.1",
        "svgicons2svgfont": "^5.0.0",
        "svg2ttf": "^4.0.0",
        "ttf2eot": "^2.0.0",
        "ttf2woff": "^2.0.0",
        "ttf2woff2": "^2.0.0",
        "nunjucks": "^2.0.0"
    },
    "devDependencies": {
        "ava": "^0.15.0",
        "babel-cli": "^6.11.0",
        "babel-core": "^6.11.0",
        "babel-plugin-espower": "^2.0.0",
        "babel-preset-es2015": "^6.9.0",
        "babel-register": "^6.9.0",
        "coveralls": "^2.11.6",
        "npm-run-all": "^2.3.0",
        "npmpub": "^3.1.0",
        "eslint": "^3.0.0",
        "eslint-plugin-ava": "^2.5.0",
        "eslint-plugin-import": "^1.10.0",
        "eslint-plugin-itgalaxy": "^6.0.0",
        "eslint-plugin-jsx-a11y": "^2.0.0",
        "eslint-plugin-lodash": "^1.9.0",
        "eslint-plugin-node": "^2.0.0",
        "eslint-plugin-objects": "^1.1.0",
        "eslint-plugin-promise": "^2.0.0",
        "eslint-plugin-react": "^5.2.0",
        "remark-cli": "^1.0.0",
        "remark-lint": "^4.0.0",
        "rimraf": "^2.5.2",
        "nyc": "^7.0.0"
    },
    "scripts": {
        "coveralls": "nyc report --reporter=text-lcov | coveralls",
        "lint:eslint": "eslint . --ignore-path .gitignore  --color",
        "lint:remark": "remark '{**/,}*.md' --ignore-path .gitignore --frail --quiet --color",
        "lint": "npm-run-all -l --parallel lint:*",
        "pretest": "npm run lint",
        "ava": "nyc ava --verbose \"src/**/__tests__/*.js\"",
        "test": "npm run ava",
        "prebuild": "rimraf dist",
        "build": "babel src --out-dir dist --ignore /__tests__/",
        "prepublish": "npm run build",
        "release": "npmpub"
    },
    "ava": {
        "require": "babel-register",
        "concurrency": 5
    },
    "nyc": {
        "exclude": [
            "**/__tests__"
        ]
    },
    "babel": {
        "presets": [
            "es2015"
        ]
    },
    "eslintConfig": {
        "rules": {
            "node/shebang": "off"
        },
        "extends": [
            "plugin:itgalaxy/ava",
            "plugin:itgalaxy/esnext",
            "plugin:itgalaxy/node"
        ],
        "root": true
    }
}
